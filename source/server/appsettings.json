{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{{InstrumentationKey}}"
  },
  "AzureAD": {
    "Audience": "{{Audience}}",
    "ClientId": "{{AppId}}",
    "ClientSecret": "{{AppSecret}}",
    "Instance": "https://login.microsoftonline.com",
    "TenantId": "{{TenantId}}"
  }
}
