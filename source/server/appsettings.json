{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AzureAD": {
    "Audience": "{{Audience}}",
    "ClientId": "{{ClientId}}",
    "ClientSecret": "{{ClientSecret}}",
    "Instance": "https://login.microsoftonline.com",
    "Scope": [
      "openid",
      "profile",
      "Channel.ReadBasic.All",
      "Group.Read.All",
      "Team.ReadBasic.All",
      "TeamMember.Read.All",
      "User.Read",
      "User.ReadBasic.All"
    ],
    "TenantId": "{{TenantId}}"
  }
}
