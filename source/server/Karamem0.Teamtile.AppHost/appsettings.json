{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Azure": {
    "SubscriptionId": "{{AZURE_SUBSCRIPTION_ID}}",
    "ResourceGroup": "{{AZURE_RESOURCE_GROUP}}",
    "Location": "{{AZURE_LOCATION}}",
    "AllowResourceGroupCreation": false
  },
  "Parameters": {
    "MicrosoftIdentityClientId": "{{MICROSOFT_IDENTITY_APP_ID}}",
    "MicrosoftIdentityClientSecret": "{{MICROSOFT_IDENTITY_APP_SECRET}}",
    "MicrosoftIdentityTenantId": "{{MICROSOFT_IDENTITY_TENANT_ID}}"
  }
}
