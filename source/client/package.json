{
  "name": "@karamem0/teamtile",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=default --reporter=junit --coverage",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss,jsx,tsx}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss,jsx,tsx}' --fix",
    "formatjs:extract": "formatjs extract 'src/**/messages.ts' --out-file 'src/translations/en.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "formatjs:compile": "npm run formatjs:compile-en && npm run formatjs:compile-ja",
    "formatjs:compile-en": "formatjs compile 'src/translations/en.json' --out-file 'src/translations/compiled/en.json' --ast --format simple",
    "formatjs:compile-ja": "formatjs compile 'src/translations/ja.json' --out-file 'src/translations/compiled/ja.json' --ast --format simple"
  },
  "dependencies": {
    "@automapper/core": "^8.8.1",
    "@automapper/pojos": "^8.8.1",
    "@azure/msal-browser": "^4.13.2",
    "@azure/msal-react": "^3.0.13",
    "@emotion/react": "^11.14.0",
    "@fluentui/react-components": "^9.66.2",
    "@fluentui/react-icons": "^2.0.304",
    "@microsoft/applicationinsights-react-js": "^18.3.6",
    "@microsoft/applicationinsights-web": "^3.3.8",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@microsoft/teams-js": "^2.39.0",
    "deepmerge": "^4.3.1",
    "dexie": "^4.0.11",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "react-intl": "^7.1.11",
    "react-router-dom": "^7.6.2",
    "react-use": "^17.6.0",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/babel-preset-css-prop": "^11.12.0",
    "@emotion/jest": "^11.13.0",
    "@emotion/react": "^11.14.0",
    "@eslint/js": "^9.29.0",
    "@formatjs/cli": "^6.7.2",
    "@microsoft/microsoft-graph-types": "^2.40.0",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-formatjs": "^10.5.39",
    "eslint": "^9.29.0",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.3",
    "eslint-plugin-testing-library": "^7.5.3",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "postcss-styled-syntax": "^0.7.1",
    "sass": "^1.89.2",
    "stylelint": "^16.20.0",
    "stylelint-config-recess-order": "^6.1.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "22.16.0"
  }
}
