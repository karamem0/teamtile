    "jest-junit": "^16.0.0",
    "postcss": "^8.4.47",
    "postcss-styled-syntax": "^0.6.4",
    "sass": "^1.79.3",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-plugin-env-compatible": "^2.0.1"
  },
  "volta": {
    "node": "20.16.0"
  }
}
