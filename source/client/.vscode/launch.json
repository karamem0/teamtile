{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Debug Web (Teams)",
      "type": "chrome",
      "request": "launch",
      "url": "https://dev.teams.microsoft.com",
      "webRoot": "${workspaceFolder}",
      "runtimeArgs": [
        "-incognito"
      ],
      "preLaunchTask": "react-start",
      "postDebugTask": "react-terminate"
    },
    {
      "name": "Debug Web",
      "type": "chrome",
      "request": "launch",
      "url": "https://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "runtimeArgs": [
        "-incognito"
      ],
      "preLaunchTask": "react-start",
      "postDebugTask": "react-terminate"
    }
  ]
}
