{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Debug Teams (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "https://teams.microsoft.com/_#/l/app/{{AppId}}?installAppPackage=true",
      "webRoot": "${workspaceFolder}",
      "runtimeArgs": [
        "-incognito"
      ],
      "preLaunchTask": "react-start",
      "postDebugTask": "react-terminate"
    },
    {
      "name": "Debug (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "https://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "runtimeArgs": [
        "-incognito"
      ],
      "preLaunchTask": "react-start",
      "postDebugTask": "react-terminate"
    }
  ]
}
